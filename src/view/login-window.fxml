<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchor" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.LoginPage">
   <children>
      <TextField fx:id="login" layoutX="227.0" layoutY="132.0" promptText="Login" />
      <Button fx:id="signInBut" layoutX="525.0" layoutY="14.0" mnemonicParsing="false" onAction="#signUp" text="Sign Up" />
      <Button fx:id="loginBut" layoutX="227.0" layoutY="247.0" mnemonicParsing="false" onAction="#logIn" prefHeight="26.0" prefWidth="150.0" text="Login" />
      <Button fx:id="forgotBut" layoutX="224.0" layoutY="342.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Forgot your password?" />
      <PasswordField fx:id="password" layoutX="227.0" layoutY="187.0" promptText="Password" />
   </children>
</AnchorPane>
